<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="complianceCheckPane" prefHeight="200" prefWidth="400" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.keyacosmetic.Shamanta.QualityControlOfficer.ComplianceCheckController">
    <children>
        <Label layoutX="14" layoutY="14" text="Start Compliance Check" />
        <TextField fx:id="dimensionField" layoutX="14" layoutY="50" promptText="Enter Dimension" />
        <TextField fx:id="weightField" layoutX="14" layoutY="90" promptText="Enter Weight" />
        <Button fx:id="submitBtn" layoutX="14" layoutY="130" onAction="#submitOA" text="Submit" />
    </children>
</AnchorPane>
