<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="uploadTestResultPane" prefHeight="250" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.keyacosmetic.Shamanta.QualityControlOfficer.TestResultController">
    <children>
        <Label layoutX="14" layoutY="14" text="Upload Test Result" />
        <TextField fx:id="batchIdField" layoutX="14" layoutY="50" promptText="Batch ID" />
        <TextField fx:id="testTypeField" layoutX="14" layoutY="90" promptText="Test Type" />
        <DatePicker fx:id="testDate" layoutX="14" layoutY="130" />
        <Button fx:id="chooseFileBtn" layoutX="14" layoutY="170" onAction="#ChooseFileOA" text="Choose File" />
        <Button fx:id="uploadBtn" layoutX="110" layoutY="170" onAction="#UploadOA" text="Upload" />
    </children>
</AnchorPane>
