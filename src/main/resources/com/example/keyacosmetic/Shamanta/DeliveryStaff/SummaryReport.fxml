<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="summaryReportPane" prefHeight="220" prefWidth="450" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.keyacosmetic.Shamanta.DeliveryStaff.SummaryReportController">
    <children>
        <Label layoutX="14" layoutY="14" text="Daily Summary Report" />
        <Button fx:id="generateReportBtn" layoutX="14" layoutY="50" onAction="#ReportOA" text="Generate Report" />
        <TextArea fx:id="summaryOutput" layoutX="14" layoutY="90" prefHeight="100" prefWidth="400" />
    </children>
</AnchorPane>
