<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="otpVerificationPane" prefHeight="150" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.keyacosmetic.Shamanta.DeliveryStaff.DeliveryVerificationController">
    <children>
        <Label layoutX="14" layoutY="14" text="OTP Verification" />
        <TextField fx:id="otpField" layoutX="14" layoutY="50" promptText="Enter OTP" />
        <Button fx:id="verifyOtpBtn" layoutX="14" layoutY="90" onAction="#VerifyOA" text="Verify" />
    </children>
</AnchorPane>
