<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="366.0" prefWidth="407.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.final_project_summer.MaintainQualityController">
    <Label layoutX="20" layoutY="20" text="Maintain Product Quality" />
    <TextField fx:id="batchIdField" layoutX="20" layoutY="60" prefWidth="200" promptText="Enter Batch ID" />
    <Button layoutX="20" layoutY="100" onAction="#handleCheckQuality" text="Check Quality" />
    <Label fx:id="qualityLabel" layoutX="20" layoutY="140" text="Quality score and defects" />
</AnchorPane>
