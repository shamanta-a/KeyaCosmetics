<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="navigationPane" prefHeight="160" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.keyacosmetic.DeliveryStaff.NavigationAssistanceController">
    <children>
        <Label layoutX="14" layoutY="14" text="Get Directions" />
        <ComboBox fx:id="deliverySelector" layoutX="14" layoutY="50" prefWidth="250" />
        <Button fx:id="getDirectionsBtn" layoutX="14" layoutY="90" onAction="#directionOA" text="Get Directions" />
    </children>
</AnchorPane>
