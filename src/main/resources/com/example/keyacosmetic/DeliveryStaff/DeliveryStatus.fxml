<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="statusChangePane" prefHeight="180" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.keyacosmetic.DeliveryStaff.DeliveryStatusController">
    <children>
        <Label layoutX="14" layoutY="14" text="Change Delivery Status" />
        <ComboBox fx:id="deliveryDropdown" layoutX="14" layoutY="50" prefWidth="200" />
        <ComboBox fx:id="statusDropdown" layoutX="14" layoutY="90" prefWidth="200" />
        <Button fx:id="updateStatusBtn" layoutX="14" layoutY="130" onAction="#updatestatusOA" text="Update Status" />
    </children>
</AnchorPane>
