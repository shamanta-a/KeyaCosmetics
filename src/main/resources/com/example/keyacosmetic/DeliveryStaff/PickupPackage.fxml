<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pickupPane" prefHeight="150" prefWidth="400" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.keyacosmetic.DeliveryStaff.PickupPackageController">
    <children>
        <Label layoutX="14" layoutY="14" text="Pickup Package" />
        <TextField fx:id="packageIdTF" layoutX="14" layoutY="50" promptText="Enter or Scan Package ID" />
        <Button fx:id="pickupBtn" layoutX="14" layoutY="90" text="Mark as Picked Up" />
    </children>
</AnchorPane>
