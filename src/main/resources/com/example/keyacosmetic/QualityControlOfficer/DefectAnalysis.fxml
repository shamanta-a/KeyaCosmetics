<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="defectAnalysisPane" prefHeight="250" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.keyacosmetic.QualityControlOfficer.DefectAnalysisController">
    <children>
        <Label layoutX="14" layoutY="14" text="Defect Analysis" />
        <TextField fx:id="batchRangeField" layoutX="14" layoutY="50" promptText="Enter Batch Range" />
        <Button fx:id="analyzeBtn" layoutX="14" layoutY="90" onAction="#analyzeOA" text="Analyze" />
        <TableView fx:id="defectTable" layoutX="14" layoutY="130" prefHeight="100" prefWidth="370" />
    </children>
</AnchorPane>
